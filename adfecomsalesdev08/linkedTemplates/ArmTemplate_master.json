{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfecomsalesdev08"
		},
		"ADLS_SQL_PAR_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_SQL_PAR'"
		},
		"LS_SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SqlServer1'"
		},
		"SL_REST_API_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SL_REST_API'"
		},
		"ADLS_SQL_PAR_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssidevecom08.dfs.core.windows.net/"
		},
		"LS_SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "LAPTOP-SHPI52Q8\\SQLEXPRESS"
		},
		"LS_SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "salesecom"
		},
		"LS_SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"LS_REST_PAR_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retoolapi.dev/pCtpVZ/data"
		},
		"SL_REST_API_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssidevecom08.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfecomsalesdev08_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_SQL_PAR_accountKey": {
						"value": "[parameters('ADLS_SQL_PAR_accountKey')]"
					},
					"SL_REST_API_accountKey": {
						"value": "[parameters('SL_REST_API_accountKey')]"
					},
					"LS_SqlServer1_password": {
						"value": "[parameters('LS_SqlServer1_password')]"
					},
					"ADLS_SQL_PAR_properties_typeProperties_url": {
						"value": "[parameters('ADLS_SQL_PAR_properties_typeProperties_url')]"
					},
					"SL_REST_API_properties_typeProperties_url": {
						"value": "[parameters('SL_REST_API_properties_typeProperties_url')]"
					},
					"LS_SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('LS_SqlServer1_properties_typeProperties_server')]"
					},
					"LS_SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('LS_SqlServer1_properties_typeProperties_database')]"
					},
					"LS_SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('LS_SqlServer1_properties_typeProperties_userName')]"
					},
					"LS_REST_PAR_properties_typeProperties_url": {
						"value": "[parameters('LS_REST_PAR_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}